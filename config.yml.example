config:
  ignored_fields:
    - branch_id
    - mcc
    - memo
    - curdef

  field_order:
    - "date"
    - "checknum"
    - "payee"
    - "amount"
    - "type"
    - "id"
    - "account_id"
    - "account_type"

  payees:
    - name: "Wawa"
      patterns:
        - "^WAWA.*"
    - name: "Amazon"
      patterns:
        - "^AMAZON.*"
        - "PwP\\s+AMAZON\\s+MKTP\\s+Privacycom"
    - name: "Weiss Markets"
      patterns:
        - '(?i)\bWEIS\s+MARKETS\b'
    - name: "CVS Pharmacy"
      patterns:
        - "CVS/PHARM.*"
    - name: "Spotify"
      patterns:
        - 'SPOTIFY\s+USA'
  logging:
    version: 1
    disable_existing_loggers: False
    formatters:
      standard:
        format: '%(asctime)s [%(levelname)s] %(name)s: %(message)s'
    handlers:
      console:
        class: logging.StreamHandler
        level: DEBUG
        formatter: standard
        stream: ext://sys.stdout
    root:
      level: DEBUG  # Set this to the desired level
      handlers:
        - console
      version: 1
      level: "DEBUG"
      format: "%(asctime)s - %(name)s - %(levelname)s - %(message)s"
